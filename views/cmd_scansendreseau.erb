<IoScanAndSend>
    <TxProfiles>
        <Cifs>
            <CifsParams>
                <FileName><%= @prmtr[:scanreseau][:filename] %></FileName>
                <Host><%= @prmtr[:scanreseau][:host_or_email] %></Host>
                <User><%= @prmtr[:scanreseau][:user] %></User>
                <AuthMethod><%= @prmtr[:scanreseau][:password] %></AuthMethod>
                <StoreDir><%= @prmtr[:scanreseau][:storedir] %></StoreDir>
            </CifsParams>
        </Cifs>
    </TxProfiles>
	<DocSize>Auto</DocSize>
	<DuplexScanEnable><%= @prmtr[:scanglobal][:duplexscanenable] %></DuplexScanEnable>
    <ScanTray>ADF</ScanTray>
    <ColorMode><%= @prmtr[:scanglobal][:colormode] %></ColorMode>
    <Resolution><%= @prmtr[:scanglobal][:resolution] %></Resolution>
    <FileType><%= @prmtr[:scanglobal][:filetype] %></FileType>
    <FileNameFixed>false</FileNameFixed>
	<SinglePageFile><%= @prmtr[:scanglobal][:singlepagefile] %></SinglePageFile>
	<AutoDeskew><%= @prmtr[:scanglobal][:autodeskew] %></AutoDeskew>
	<SkipBlankPage><%= @prmtr[:scanglobal][:skipblankpage] %></SkipBlankPage>
	<ShortEdgeBinding><%= @prmtr[:scanglobal][:shortedgebinding] %></ShortEdgeBinding>
    <JobFinAckUrl><%= @prmtr[:scanglobal][:submit] %></JobFinAckUrl>
</IoScanAndSend>
